{
  "name": "blocdash-nextjs-server",
  "version": "1.0.0",
  "description": "blocdash",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "server": "nodemon dist/index.js",
    "start-ts": "ts-node src/index.ts",
    "server-ts": "nodemon --exec ts-node src/index.ts",
    "watch": "tsc -w",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:generate": "npx typeorm-ts-node-esm migration:generate src/migrations -d src/db/dataSourceProd.ts",
    "migration:run": "yarn typeorm migration:run -d src/db/dataSourceProd.ts"
  },
  "author": "DN",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/node": "^22.13.1",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@mui/material": "^6.4.4",
    "@typegoose/typegoose": "^12.11.0",
    "@types/uuid": "^10.0.0",
    "apollo-server-core": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "argon2": "^0.41.1",
    "axios": "^1.7.9",
    "class-validator": "^0.14.1",
    "connect-mongo": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.17.1",
    "express-session": "^1.18.1",
    "graphql": "^16.10.0",
    "graphql-scalars": "^1.24.1",
    "mongodb": "^6.13.0",
    "mongoose": "~8.10.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.10.0",
    "pg": "^8.13.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "type-graphql": "^2.0.0-rc.2",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5"
  }
}
